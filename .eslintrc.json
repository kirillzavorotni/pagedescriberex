{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": ["error", 4],
    "max-len": ["error", {"code": 120}],
    "linebreak-style": 0,
    "no-await-in-loop": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0,
    "lines-between-class-members": 0
  }
}
